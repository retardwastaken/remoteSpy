return local function create(instance,properties)
    if not instance then
        print('error while trying to create instance',instance,typeof(properties) == 'table' and unpack(properties) or properties)
    return end

    local obj = Instance.new(instance)
    
    if not properties or properties == {} then
        print('error while trying to create instance',instance,typeof(properties) == 'table' and unpack(properties) or properties)
    return end

    for i,v in pairs(properties) do
        obj[i] = v
    end
    return obj
end
